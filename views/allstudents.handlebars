<div>All Student List</div>

<div class = "container ">

<div class = "col-md-8 ">
   <table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>
  <tbody>
    {{#each student}} 
        <tr> 
            <td>{{first_name}}</td> 
           <td>{{last_name}}</td>
        </tr> 
        {{/each}}

  </tbody>
</table>
</div>

<a class="btn btn-success " href="/">Done</a>

</div>


